#include<stdio.h>
void main()
{
    int a,b;
    printf("enter value of first number = ");
    scanf("%d",&a);
    printf("enter value of second number = ");
    scanf("%d",&b);
    
    if(a>b)
    {
        printf("%d is greater than %d\n",a,b);
    }
    else printf("%d is greater than %d\n",b,a);
    
    if(a%2==0)
    {
        printf("%d is an even number \n",a);
    }
    else printf("%d is and odd number\n",a);
    
    if(b%2==0)
    {
        printf("%d is an even number\n",b);
    }
    else printf("%d is and odd number\n",b);
}
