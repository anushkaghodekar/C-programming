#include<stdio.h>
int main()
{
 char ch;
 printf("Enter any character = ");
 scanf("%c",&ch);
 if(ch<=90 && ch>=65)
 {
 printf("The entered character is a Capital letter");
 }
 else if(ch<=122 && ch>=97)
 {
 printf("The entered character is a Small letter");
 }
 else if(ch<=57 && ch>=48)
 {
 printf("The entered character is a Number between 0-9");
 }
 else if ((ch<=47 && ch>=0) || (ch<=58 && ch>=64) || (ch<=96 && ch>=91) || (ch<=123 &&
ch>=127))
 {
 printf("The entered character is special symbol");
 }
 else
 {
 printf("Invalid input");
 }
return 0;
}
