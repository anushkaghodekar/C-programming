#include<stdio.h>
void swap(int,int);
void main()
{
    int num1,num2;
    printf("enter value of first number = ");
    scanf("%d",&num1);
    printf("enter value of second number = ");
    scanf("%d",&num2);
    printf("before swapping, 1stnum=%d & 2ndnum=%d \n",num1,num2);
    swap(num1,num2);
  
}
void swap(int num1,int num2)
{
    int temp;
    temp=num1;
    num1=num2;
    num2=temp;
    printf("after swapping 1stnum=%d & 2ndnum=%d",num1,num2);
}
