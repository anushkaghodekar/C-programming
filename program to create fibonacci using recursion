#include<stdio.h>
int fibo(int n);
int main()
{
 int c,n;
 printf("Enter the number of terms = ");
 scanf("%d",&n);
 printf("Fibbonachi Series is as follows : ");
 for(c=0;c<n;c++)
 {
 printf(" %d ",fibo(c));
 }

return 0;
}
int fibo(int n)
{
 if(n==0)
 return 0;
 else if(n==1)
 return 1;
 else
 return(fibo(n-1)+fibo(n-2));

}
