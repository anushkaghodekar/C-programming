#include<stdio.h>
int Fibonacci(int n);
void main()
{
    int n,c;
    printf("enter any value of n = ");
    scanf("%d",&n);
    printf("fibonacci series is as follows = ");
    
    for(c=0;c<n;c++)
    {
        printf("%d\t",Fibonacci(c));
    }
}
int Fibonacci(int n)
{
    if(n==0)
    return 0;
    else if(n==1)
    return 1;
    else return Fibonacci(n-1)+Fibonacci(n-2);
    
}
